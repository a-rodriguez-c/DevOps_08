{
    "executionRoleArn": "arn:aws:iam::093066193290:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ],

            "image": "<IMAGE1_NAME>",
            "essential": true,
            "name": "Container-blacklist"
        }
    ],

    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "Task-Blacklist"
}